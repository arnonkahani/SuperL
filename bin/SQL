BEGIN TRANSACTION;
CREATE TABLE `WorkerSchedule` (
	`WID`	TEXT NOT NULL,
	`Day`	TEXT NOT NULL CHECK(Day IN ( 'Sunday' , 'Monday' , 'Tuesday' , 'Wednesday' , 'Thursday' , 'Friday' , 'Saturday' )),
	`Type`	TEXT NOT NULL,
	PRIMARY KEY(WID,Day,Type),
	FOREIGN KEY(WID) REFERENCES `Worker`(`ID`) ON UPDATE CASCADE ON DELETE CASCADE
);
CREATE TABLE `WorkerInShifts` (
	`WID`	TEXT NOT NULL,
	`Day`	TEXT NOT NULL CHECK(Day IN ( 'Sunday' , 'Monday' , 'Tuesday' , 'Wednesday' , 'Thursday' , 'Friday' , 'Saturday' )),
	`Type`	TEXT NOT NULL,
	`Date`	TEXT NOT NULL,
	`Ability`	TEXT NOT NULL,
	PRIMARY KEY(WID,Day,Type),
	FOREIGN KEY(WID) REFERENCES `Worker`(`ID`) ON UPDATE CASCADE ON DELETE CASCADE
);
CREATE TABLE `WorkerAbilities` (
	`WID`	TEXT NOT NULL,
	`Ability`	TEXT NOT NULL,
	PRIMARY KEY(WID,Ability)
	FOREIGN KEY(WID) REFERENCES `Worker`(`ID`) ON UPDATE CASCADE ON DELETE CASCADE);
INSERT INTO `WorkerAbilities` (WID,Ability) VALUES ('000000000','HRManager');
CREATE TABLE `Worker` (
	`ID`	TEXT,
	`BankNO`	TEXT NOT NULL UNIQUE,
	`EmploymentDate`	TEXT NOT NULL,
	`EmploymentTerms`	TEXT,
	`Name`	TEXT NOT NULL,
	PRIMARY KEY(ID)
);
INSERT INTO `Worker` (ID,BankNO,EmploymentDate,EmploymentTerms,Name) VALUES ('000000000','000000000000','01-01-2000','Hard coded Admin','Admin');
CREATE TABLE `WeeklyShift` (
	`WID`	TEXT NOT NULL,
	`Day`	TEXT NOT NULL,
	`Type`	TEXT NOT NULL,
	`Ability`	TEXT NOT NULL,
	PRIMARY KEY(WID,Day,Type)
);
CREATE TABLE WEEKLY_ORDER_PRODUCT (DAY CHAR(50)      NOT NULL,ID INTEGER      NOT NULL,AMOUNT INT     NOT NULL,FOREIGN KEY (DAY) REFERENCES WEEKLY_ORDER(DAY),FOREIGN KEY (ID) REFERENCES PRODUCT(ID),PRIMARY KEY (DAY ,ID));
CREATE TABLE WEEKLY_ORDER (DAY CHAR(50)      NOT NULL,PRIMARY KEY (DAY));
CREATE TABLE `User` (
	`WID`	TEXT,
	`Username`	TEXT NOT NULL UNIQUE,
	`Password`	TEXT NOT NULL,
	PRIMARY KEY(WID),
	FOREIGN KEY(WID) REFERENCES `Worker`(`ID`) ON UPDATE CASCADE ON DELETE CASCADE
);
INSERT INTO `User` (WID,Username,Password) VALUES ('000000000','Admin','Admin');
CREATE TABLE Truck (LicenceNumber TEXT PRIMARY KEY NOT NULL,LicenceType TEXT,Model TEXT,Color TEXT,MaxWeight REAL,NetWeight REAL);
INSERT INTO `Truck` (LicenceNumber,LicenceType,Model,Color,MaxWeight,NetWeight) VALUES ('1234567893','A','mitsubishi','blue',1000.0,200.0);
INSERT INTO `Truck` (LicenceNumber,LicenceType,Model,Color,MaxWeight,NetWeight) VALUES ('987654321','B','ford','red',2000.0,400.0);
INSERT INTO `Truck` (LicenceNumber,LicenceType,Model,Color,MaxWeight,NetWeight) VALUES ('312879465','C','folkswagen','greeb',1500.0,100.0);
CREATE TABLE Transportation (ID INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,StartTime DATETIME,EndTime DATETIME,Description TEXT,truck TEXT,driver TEXT,area INTEGER,arrived INTEGER,FOREIGN KEY(driver) REFERENCES Driver(WID) ON DELETE RESTRICT ON UPDATE CASCADE ,FOREIGN KEY(truck) REFERENCES Truck(LicenceNumber) ON DELETE RESTRICT ON UPDATE CASCADE ,FOREIGN KEY(area) REFERENCES Area(areaID) ON DELETE RESTRICT ON UPDATE CASCADE );
CREATE TABLE `ShiftsHistory` (
	`WID`	TEXT NOT NULL,
	`Day`	TEXT NOT NULL,
	`Type`	TEXT NOT NULL,
	`Date`	TEXT NOT NULL,
	`Ability`	TEXT NOT NULL,
	PRIMARY KEY(WID,Day,Type)
);
CREATE TABLE `ShiftManager` (
	`MID`	TEXT NOT NULL,
	`Day`	TEXT NOT NULL CHECK(Day IN ( 'Sunday' , 'Monday' , 'Tuesday' , 'Wednesday' , 'Thursday' , 'Friday' , 'Saturday' )),
	`Type`	TEXT NOT NULL,
	`Date`	TEXT NOT NULL,
	`Ability`	TEXT NOT NULL,
	PRIMARY KEY(MID,Day,Type),
	FOREIGN KEY(MID) REFERENCES `Worker`(`ID`) ON UPDATE CASCADE ON DELETE CASCADE
);

INSERT INTO `SUPPLY_AGREEMENT_PRODUCT` (SUPPLY_AGREEMENT_ID,SUPPLIER_PRODUCT_SN,PRICE) VALUES (1,1,10.0);
INSERT INTO `SUPPLY_AGREEMENT` (ID,SUPPLYTYPE,DELEVERYTYPE,DAY,SUPPLIERCN) VALUES (1,'ondemand','cometake','0','1234');
